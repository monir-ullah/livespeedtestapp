{
  "manifest_version": 3,
  "name": "Error Beautifier",
  "version": "1.0",
  "description": "Extract and beautify errors from web pages for readability.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
